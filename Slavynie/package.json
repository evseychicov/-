{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "build": "electron-builder --publish=never",
    "test": "npm start"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "build": {
    "appId": "Slavynie",
    "productName": "Slavynie",
    "win": {
      "asar": false,
      "target": [
        "nsis"
      ],
      "icon": "build/icon.ico"
    },
    "directories": {
      "output": "build"
    }
  },
  "devDependencies": {
    "electron": "^3.0.7",
    "electron-builder": "^20.38.4"
  },
  "dependencies": {
    "bootstrap": "^4.1.3",
    "handlebars": "^4.0.12",
    "jquery": "^3.3.1",
    "moment": "^2.23.0",
    "pg-promise": "^8.5.4",
    "popper.js": "^1.14.3",
    "tableexport": "^5.2.0"
  }
}
